?L10:	SET 'NUM,OCNT
	JUMP ?L12
?L7:	GRTR? ICNT,1 \?L11
	SET 'PTBL,0
	SET 'TBL,P-PRSI
	GET P-PRSO,1 >OBJ
	SET 'NUM,ICNT
	JUMP ?L12
