	ICALL1 KILL-SOUNDS
	ZERO? GL-DEBUG-FLAG /?L1
	ICALL1 V-REFRESH
?L1:	ICALL1 RT-P-CLEAR
	PUTB GL-OOPS-INBUF,1,0
